<ul>
  <% result[:items].each do |r| %>
    <li>
      <a href="<%= r.url %>" target="_blank">
        <div class="tweet">
          <h4><%= r.username %></h4>
          <p><%= r.content %></p>
        </div>
      </a>
    </li>
  <% end %>
  <% if result[:items].length == 0 %>
    <li>No results</li>
  <% end %>
</ul>
