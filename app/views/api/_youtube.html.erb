<ul>
	<% result["youtube"][:items].each do |r| %>
		<li>
			<% if r.type == 'youtube#video' %>
				<%= r.description %>
			<% elsif r.type == 'youtube#playlist' %>
				<%= r.description %>
			<% else %>
				<iframe height="265" src="//www.youtube.com/embed/<%= r.id %>" frameborder="0" allowfullscreen></iframe>
			<% end %>
		</li>
	<% end %>
	<% if result["youtube"][:items].length == 0 %>
		<li>No results</li>
	<% end %>
</ul>