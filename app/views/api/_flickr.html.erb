<ul>
	<% result["flickr"][:items].each do |r| %>
		<li nextpagetoken="<%= result['flickr'][:page] %>">
			<a href="<%= r.url %>" target="_blank">
			<div class="flickr-image">
				<img src=<%= r.content %> class="flickr-image" align="center" />
				<div class="flickr-content" style="display:none;">
					<span class="title"><%= r.title %></span>
					<p><%= r.description.html_safe %></p>
				</div>
			</div>
			</a>
		</li>
	<% end %>
	<% if result["flickr"][:items].length == 0 %>
		<li class="error">No results</li>
	<% end %>
</ul>
